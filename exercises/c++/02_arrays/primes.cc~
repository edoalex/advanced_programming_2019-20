#include <iostream>

bool is_prime(unsigned int n){
  
  for(unsigned int divisor=2; divisor<n; divisor++){
    if(n%divisor == 0){
      return False;}}
  return True;
}


int main(){
  unsigned int primes[50];
  unsigned int cont=1;
  unsigned int candidate=3;
  primes[0] = 2;
  
  while(candidate<100){
    if(is_prime(candidate)){
      primes[cont] = candidate;
      cont++;}
    candidate++;}
  std::cout << primes;

  return 0;}
