#include <iostream>
#include <cmath>

int main(){
  std::cout << "Insert number: \n";
  unsigned int n;
  std::cin >> n;
  bool primes{new [n]};

  while(unsigned int i=0; i<=n; i++){
    primes[i] = true;}

  for(unsigned int i=2; i <= std::sqrt(n); i++){
    if(primes[i]){
      for(j=i*i, j<=n; j = j + i){
	primes[j] = false;}
    }
  }

  while(unsigned int i=0; i<=n; i++){
    if(primes[i]){
      std::cout << i << std::endl;
    }
  }


  return 0;}
